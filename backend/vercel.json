{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "builds": [
        { 
            "src": "dist/index.ts", 
            "use": "@vercel/node" 
        },
        {
            "src":"src/**/*",
            "use":"@vercel/static"
        }
    ],
    "routes": [
        { 
            "src": "/api/(.*)", "dest": "dist/index.ts"
        }
    ]
}